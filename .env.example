NODE_ENV=development

# Database Configuration
DB_HOST=db
DB_PORT=3306
DB_USER=cliniquoo_user
DB_PASSWORD=cliniquoo_password
DB_NAME=cliniquoo_db

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# JWT & Security
JWT_SECRET=dev-jwt-secret-key-change-in-production
ENCRYPTION_KEY=dev-encryption-key-change-in-production

# AWS Configuration
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=fake-key
AWS_SECRET_ACCESS_KEY=fake-secret
AWS_S3_BUCKET=cliniquoo-dev

# API Keys
OPENAI_API_KEY=sk-test-key
GOOGLE_API_KEY=test-key
RAZORPAY_KEY_ID=test-key
RAZORPAY_KEY_SECRET=test-secret

# Application
PORT=3000
APP_NAME=Cliniquoo Backend
APP_VERSION=1.0.0
# Feature Configuration
REMINDER_SERVICE_ENABLED=true
REMINDER_MAX_RETRIES=3
REVENUE_CACHE_TTL=3600
REVENUE_MAX_RANGE_DAYS=365
IMPORT_CHUNK_SIZE=100
MAX_IMPORT_ROWS=10000
MEDICINE_INTERACTION_CHECK_ENABLED=true
MEDICINE_INTERACTION_CACHE_TTL=1800
SUBSCRIPTION_FEATURE_GATES_ENABLED=true
SUBSCRIPTION_FEATURE_CACHE_TTL=1800
EMAIL_NOTIFICATION_ENABLED=true
EMAIL_BATCH_SIZE=50
EMAIL_RETRY_STRATEGY=exponential